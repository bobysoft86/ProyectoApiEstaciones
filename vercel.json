{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/api/user(.*)",
            "dest": "/api/user"
        },
        {
            "src": "/api/estaciones(.*)",
            "dest": "/api/estaciones"
        },
        {
            "src": "/api/user/:id(.*)",
            "dest": "/api/user/:id"  
        },
        {
            "src": "/api/user/register(.*)",
            "dest": "/api/user/register"  
        },
        {
            "src": "/api/estaciones/:id(.*)",
            "dest": "/api/estaciones/:id"  
        },
        {
            "src": "/api/user/register/:id(.*)",
            "dest": "/api/user/register/:id"  
        },
        {
            "src": "/api/user/authenticate(.*)",
            "dest": "/api/user/authenticate"
        },
        {
            "src": "/api/messages(.*)",
            "dest": "/api/messages"
        }
    ]
}